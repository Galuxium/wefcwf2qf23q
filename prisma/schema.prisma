prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int       @id @default(autoincrement())
  email     String    @unique
  password  String
  projects  Project[]
  subscriptions Subscription[]
}

model Project {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  user        User     @relation(fields: [userId], references: [id])
  userId      Int
}

model Subscription {
  id    Int    @id @default(autoincrement())
  type  String
  user  User   @relation(fields: [userId], references: [id])
  userId Int
}